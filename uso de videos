**video 04**
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <br>
    <video id="video play"width="400" controls="controns">
        <source src="./videos/moto.mp4.mp4"type="video/mp4"type="video/mp4">
        <source src="oi.flv"type="video/flv">
    </video>
    <div>
        <script> 
        var myvideo=document.getElementById("video play");
        function makeBig(){
            myvideo.width="1650";
        }
        function makeSmall(){
            myvideo.width="190";

        }
        function makeNormal(){
            myvideo.width="910";

        }

        </script>
        <div style="text-align: left;"></div>
        <button onclick="makeBig()"> Aumentar</button>
        <button onclick="makeSmall()"> minimizar </button>
        <button onclick="makeNormal()"> normal</button>
        <p></p>
        </div>
    </body>
</html>


